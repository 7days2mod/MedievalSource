        <poi name="Water" poimapcolor="#0063ff" blockname="water" blockbelow="sand" yposfill="58"/>
        <poi name="WaterDesert" poimapcolor="#002868" blockname="water" blockbelow="sand" yposfill="44"/>
        <poi name="WaterHills" poimapcolor="#5d9bfd" blockname="water" blockbelow="sand" yposfill="73"/>
        <poi name="WaterMountains" poimapcolor="#a3c7fe" blockname="water" blockbelow="sand" yposfill="78"/>
        <poi name="Sand" poimapcolor="#c6b9a2" blockname="sand"/>
        <poi name="Empty" poimapcolor="#f6ff00"/>
      </pois>
    </append>
  </config>
</configs>
